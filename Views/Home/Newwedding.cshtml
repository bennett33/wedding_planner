@model Wedding

<div class="navi-bar d-flex justify-center">
    <h1>Plan Your Wedding</h1>
</div>

<form asp-action="CreateWedding" asp-controller="Home" method="post">
        <p>
            <label asp-for="WedderOne"></label>
            <span asp-validation-for="WedderOne"></span>
            <input asp-for="WedderOne" type="text">
        </p>
        <p>
            <label asp-for="WedderTwo"></label>
            <span asp-validation-for="WedderTwo"></span>
            <input asp-for="WedderTwo" type="text">
        </p>
        <p>
            <label asp-for="WeddingDate"></label>
            <span asp-validation-for="WeddingDate"></span>
            <input asp-for="WeddingDate" type="date">
        </p>
        <p>
            <label asp-for="Address"></label>
            <span asp-validation-for="Address"></span>
            <input asp-for="Address" type="text">
        </p>
        <div>
            <button class ="btn btn-success mt-3">Create</button>
        </div>
    </form> 